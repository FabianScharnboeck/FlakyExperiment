<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE experiment>
<experiment>
  <setup>
    <configurations>
      <global> <!-- Configuration for Pynguin - 1 -->
        <option key="constant_seeding" value="True"/>
        <option key="seeded_primitives_reuse_probability" value="0.2"/>
        <option key="dynamic_constant_seeding" value="True"/>
        <option key="seeded_dynamic_values_reuse_probability" value="0.6"/>
        <option key="maximum_search_time" value="600"/>
      </global>
      <configuration id="DynaMOSA-TypeHints"> <!-- Configuration for Pynguin - 2: Put in a custom configuration name here -->
        <option key="algorithm" value="DYNAMOSA"/> <!-- Choose between the algorithms Pynguin provides -->
        <option key="type_inference_strategy" value="TYPE_HINTS"/> <!-- Use options Pynguin provides, e.g. type_inference_strategy -->
      </configuration>
    </configurations>
    <output-variables>
        <output-variable>Coverage</output-variable>
        <output-variable>CoverageTimeline</output-variable>
    </output-variables>
  </setup>
</experiment>
